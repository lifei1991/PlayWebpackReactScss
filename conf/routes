# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample index page
#GET     /index                           controllers.HomeController.index

# Map static resources from the /public folder to the /assets URL path
GET     /public/*file               controllers.Assets.versioned(path="/public", file: Asset)

# An example controller showing a sample login page
GET     /login                          controllers.HomeController.login

# An example controller showing a sample homepage
GET     /homePage                          controllers.HomeController.homePage

# An example controller showing a sample homepage
GET     /users                          controllers.HomeController.users

# An example controller showing a sample homepage
GET     /rules                          controllers.HomeController.rules

# An example controller showing a sample homepage
GET     /roles                          controllers.HomeController.roles

# An example controller showing a sample homepage
GET     /wells                          controllers.HomeController.wells
